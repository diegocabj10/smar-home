@section Scripts {
    <script src="~/Scripts/ConfiguracionController.js"></script>
}
    <div ng-app="myApp" ng-controller="myCtrl as vm">

            <label> Seleccionar Arduino:  </label>
                <select ng-model="vm.dto.Id_Arduino" ng-options="item.id as item.valor for item in vm.arduinos"></select>
            </div>

            <div>
                <label> Seleccionar Señal:  </label>
                <select ng-model="vm.dto.Id_Id_Senal" ng-options="item.id as item.valor for item in vm.senal"></select>
            </div>
            <div>
                <label>Retardo para informar Alarma:  </label> <input ng-model="vm.dto.n_delay_alarma" />
            </div>
            <div class="botones">
                <button class="btn btn-primary" ng-click="vm.Agregar_Delay();"><span class="glyphicon glyphicon-save"> </span> Guardar</button>
            </div>
        </div>
    </div>

